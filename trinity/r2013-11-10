module-whatis   "loads the Trinity RNAseq package"

# for Tcl script use only
set     version         r2013-11-10
set     appname         trinity
set     prefix          /export/apps/${appname}/${version}
set     exec_prefix     ${prefix}

conflict	trinity

prepend-path    PATH            ${exec_prefix}

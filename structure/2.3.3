##
## structure modulefile
## By Alan Orth
##
proc ModulesHelp { } { 
        global version prefix

        puts stderr "\tstructure - loads the structure population genetics package"
        puts stderr "\n\tThis adds $prefix/* to several of the"
        puts stderr "\tenvironment variables."
        puts stderr "\n\tVersion $version\n"
}

module-whatis   "loads the structure population genetics package"

# for Tcl script use only
set     version         2.3.3
set     prefix          /export/apps/structure/${version}
set     exec_prefix     ${prefix}

conflict    structure

prepend-path    PATH            ${exec_prefix}/bin
